import java.util.*;

class Program {
  public int majorityElement(int[] array) {
    // Write your code here.
    int avg = array.length / 2;
    Map<Integer, Integer> map = new HashMap<>();

    for(int i=0; i<array.length; i++){
      map.put(array[i], 0);
    }
    for(int key: array){

        
      if(map.containsKey(key)){
        map.put(key, map.get(key) + 1);
      }
      if(map.get(key) > avg){
        return key;
      }
 
    }
    
      
    
    return -1;
  }
}
